{
  "name": "react-async-elements",
  "version": "0.4.0",
  "author": "Jared Palmer <jared@palmer.net>",
  "repository": "palmerhq/react-async-elements",
  "keywords": [
    "react",
    "react-dom",
    "suspense"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "source": "src/index.js",
  "files": [
    "dist",
    "Audio.js",
    "Video.js",
    "Img.js",
    "Script.js"
  ],
  "scripts": {
    "start": "NODE_ENV=development tsc -w",
    "build": "rimraf dist && NODE_ENV=production tsc",
    "format": "prettier --trailing-comma es5 --single-quote --write 'src/**/*' 'exampe/src/**/*' 'example/README.md' 'README.md'",
    "test": "echo yolo",
    "prepare": "npm run build",
    "deploy": "np",
    "precommit": "lint-staged",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  },
  "devDependencies": {
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "np": "^3.0.4",
    "prettier": "^1.14.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "rimraf": "^2.6.2",
    "tsconfig-palmerhq": "^1.0.2",
    "tslint-config-palmerhq": "^1.0.2",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "@types/react": "^16.4.13",
    "@types/react-dom": "^16.0.7",
    "simple-cache-provider": "^0.7.0",
    "tslint": "^5.11.0"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "npm run format",
      "npm run lint",
      "git add"
    ]
  },
  "prettier": {
    "trailingComma": "es5",
    "singleQuote": true,
    "semi": true
  }
}
